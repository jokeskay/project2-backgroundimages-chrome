{
"name": "change background for chrome pages",
"version": "1.0",
"manifest_version" : 2,

"description": "Pick from a collection of cool backgrounds",
"icons": {
  "128": "Moon-PNG-Photo.png"
},

"browser_action": {
  "default_title":"change background for chrome pages",
  "default_icon":"Moon-PNG-Photo.png",
  "default_popup":"popup.html"
},
"permissions": [
  "http://*/*", 
  "https://*/*"
               ]
}
