{
"name": "change background for chrome pages",
"version": "1.0",
"manifest_version" : 2,

"description": "Pick from a collection of cool backgrounds",
"icons": {
  "128": "mickeyMswag.jpg"
},

"browser_action": {
  "default_title":"change background for chrome pages",
  "default_icon":"mickeyMswag.jpg",
  "default_popup":"popup.html"
},
"permissions": [
  "http://*.google.com/", 
  "https://*.google.com/",
  "storage"
               ]
}
